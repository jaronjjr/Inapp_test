<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Index page</title>
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
    <script>
        function getDetails() {  
    $.ajax({  
        type: "GET",  
        contentType: "application/json; charset=utf-8",  
        url: "http://localhost:3030/getAllNote", //Default.aspx is page and GetData is the WebMethod  
        data: {},  
        dataType: "json",  
        success: function(data) {  
            $('#dataTables tbody').remove(); // Every time I am removing the body of Table and applying loop to display data  
            console.log(data,"DATA OF AJAAX");    
            for (var i = 0; i < data.count; i++) {  
                $("#dataTables").append(  
                    "<tr><td>" + data.rows[i].title + "</td><td>" + data.rows[i].description + "</td>" +  
                    "<td>" + data.rows[i].date + "</td>" +   
                    "<td>" + "<input type='button' class='btn btn-primary editButton' data-id='" + data.rows[i].id + "' data-toggle='modal' data-target='#myModal' name='submitButton' id='btnEdit' value='Edit' />" + "</td>" +  
                    "<td><input type='button' class='btn btn-primary' name='submitButton' id='btnDelete' value='Delete'/> </td></tr>");  
            }  
        },  
        error: function() {  
            alert("Error while Showing update data");  
        }  
    });  
}
    </script>

  </head>
  <body>
    <h1>This is index page!</h1>
    <button onclick="getDetails()">BUTTON view</button>
    
    <table id="dataTables" class="table table-striped table-bordered table-hover dataTable no-footer" role="grid" aria-describedby="dataTables_info">  
        <thead>  
            <tr role="row">  
                <th class="sorting_asc" tabindex="0" aria-controls="dataTables" rowspan="1" colspan="1" style="width: 175px;" aria-sort="ascending" aria-label="Rendering engine: activate to sort column descending">TITLE</th>  
                <th class="sorting" tabindex="0" aria-controls="dataTables" rowspan="1" colspan="1" style="width: 203px;" aria-label="Browser: activate to sort column ascending">DESCRIPTION</th>  
                <th class="sorting" tabindex="0" aria-controls="dataTables" rowspan="1" colspan="1" style="width: 184px;" aria-label="Platform(s): activate to sort column ascending">DATE</th>   
            </tr>  
        </thead>  
        <tbody>  
            
                <tr class="gradeA odd" role="row">  
                    <td class="sorting_1">  
                       Note title
                    </td>  
                    <td>  
                        Note Description
                    </td>  
                    <td>  
                       Date
                    </td>  
                    <td>  
                        <input type="button" class="btn btn-primary editButton"   data-toggle="modal" data-target="#myModal" name="submitButton" id="btnEdit" value="Edit" />  
                    </td>  
                    <td>  
                        <input type="button" class="btn btn-primary" name="submitButton" id="btnDelete" value="Delete" />  
                    </td>  
                </tr>  
             
        </tbody>  
    </table> 
  </body>
</html>